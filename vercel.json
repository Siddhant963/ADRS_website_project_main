{
     "functions": {
       "Component/**/*.php": {
         "runtime": "vercel-php@0.7.3"
       },
       "Controller/**/*.php": {
         "runtime": "vercel-php@0.7.3"
       },
       "Model/**/*.php": {
         "runtime": "vercel-php@0.7.3"
       }
     },
     "routes": [
       {
         "src": "/(.*)",
         "dest": "/Component/$1.php"
       }
     ]
   }
   